package uk.threepp.plusscript.error;

public class PlusError {

	PlusErrorCode errorCode;
	String errorText;
	
	public PlusError(PlusErrorCode errorCode, String errorText) {
		
		this.errorCode = errorCode;
		this.errorText = errorText;
	}

	public PlusErrorCode getErrorCode() {
		return errorCode;
	}

	public String getErrorText() {
		return errorText;
	}
}
